# .env.example - Variables d'environnement (TEMPLATE)

# ⚠️ ATTENTION : Ne mettez PAS les vraies valeurs ici !
# Créez un fichier .env local avec vos vraies valeurs
# Le fichier .env est ignoré par .gitignore

# Configuration Flask
FLASK_ENV=development
FLASK_DEBUG=False

# Clés AWS (Exemples - À remplacer)
AWS_SECRET_KEY=votre_aws_key_ici
AWS_ACCESS_KEY=votre_aws_access_key_ici

# Base de données
DATABASE_PASSWORD=votre_password_db_ici
DATABASE_USER=admin
DATABASE_HOST=localhost

# Configuration du serveur
SERVER_PORT=5000
SERVER_HOST=localhost

# Clés secrètes
SECRET_KEY=votre_secret_key_ici
API_KEY=votre_api_key_ici
